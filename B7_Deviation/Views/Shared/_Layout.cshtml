    <script>

        $(document).ready(function () {
            $.when($.ajax(CheckIsLogin())).then(
                //$.ajax(CheckISPIC())
            );
            UserLoginXd
        });

        $(document).ready(function () {
            $('#spanLogout').click(function () {
                var url = "../Login/Index";

                swal({
                    title: "Logout!",
                    text: "Anda sudah berhasil Logout ",
                    type: "success",
                    showConfirmButton: !1
                });

                setTimeout(function () {
                    $(location).attr('href', url);
                }, 2000);
            });
        });

        // Set Time Out when cursor idle and no click
        $(function () {
            $("body").on('click keypress', function () {
                $('#ModalTimeOut').modal('hide');
                ResetThisSession();
            });

            $("body").mousemove(function () {
                $('#ModalTimeOut').modal('hide');
                ResetThisSession();
            });
        });

        var timeInSecondsAfterSessionOut = 10; // change this to change session time out (in seconds).
        var secondTick = 0;
        var testTime = 1500;//@(Session.Timeout) * 60;


        function ResetThisSession() {
            secondTick = 0;
        }

        function StartThisSessionTimer() {
            secondTick++;
            //var timeLeft = ((timeInSecondsAfterSessionOut - secondTick) / 60).toFixed(0); // in minutes
            var timeLeft = testTime - secondTick;

            if (secondTick >= (testTime - 10) && timeLeft > 0) {
                var msg = "Session Login anda akan berakhir dalam " +
                    timeLeft + " detik! ";

                var msg2 = " Silahkan gerakan mouse anda / akan diarahkan ke halaman login !";
                $('#msgModal').html(msg + '<br>' + msg2);
                $('#ModalTimeOut').modal('show');
            }

            //toastr.success("Time : " + timeLeft, "Top Right", {

            //    closeButton: !0,
            //    debug: !1,
            //    newestOnTop: !0,
            //    progressBar: !0,
            //    positionClass: "toast-top-right",
            //    preventDuplicates: !0,
            //    onclick: null,
            //    showDuration: "300",
            //    hideDuration: "1000",
            //    extendedTimeOut: "1000",
            //    showEasing: "swing",
            //    hideEasing: "linear",
            //    showMethod: "fadeIn",
            //    hideMethod: "fadeOut",
            //    tapToDismiss: !1
            //});

            if (secondTick > testTime) {

                clearTimeout(tick);
                window.location = "../Login/Index";
                return;
            }
            tick = setTimeout("StartThisSessionTimer()", 1000);
        }

        //StartThisSessionTimer();


        function CheckIsLogin() {
            var IsLogin = '@Request.RequestContext.HttpContext.Session["fullname"]';
            var IsRole = '@Request.RequestContext.HttpContext.Session["role"]';
            var url = "../Login/Index";

            if (IsLogin === '' || IsLogin === null) {

                swal({
                    title: "UnVerified!",
                    text: "Session Login berakhir, silahkan login kembali!",
                    type: "error",
                    showConfirmButton: !1
                });

                setTimeout(function () {
                    $(location).attr('href', url);
                }, 2000);
            }
            $('#UserLoginXd').text(IsLogin);
        }
    </script>


